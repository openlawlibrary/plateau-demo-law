{
    "repositories": {
        "openlawlibrary/plateau-demo-law-xml": {
            "custom": {
                "type": "xml",
                "allow-unauthenticated-commits": true
            }
        },
        "openlawlibrary/plateau-demo-law-html": {
            "custom": {
                "type": "html",
                "allow-unauthenticated-commits": true
            }
        },
        "openlawlibrary/law-static-assets": {
            "custom": {
                "type":"static-assets",
                "allow-unauthenticated-commits":true,
                "serve":"latest",
                "location_regex":"~.+(_document|_reader).+$"
            }
        }
    }
}